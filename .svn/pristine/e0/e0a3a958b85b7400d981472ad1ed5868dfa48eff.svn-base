//状态基类
abstract class StateBase
{
	//归属于哪个实体
	protected readonly owner:Entity;

	//状态
	public abstract get state():FSMState;

	//构造
	public constructor(owner:Entity)
	{
		this.owner = owner;
	}

	//进入状态
	public OnEnter(args:any[]):void {}

	//执行状态
	public OnExcuter(deltaTime:number):void {}

	//离开状态
	public OnLeave():void {}
}